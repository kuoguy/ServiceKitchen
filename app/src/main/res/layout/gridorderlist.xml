<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/orderticket2">

    <TextView
        android:id="@+id/textViewNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="4sp" />

    <TextView
        android:id="@+id/textViewID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textViewNumber"
        android:layout_alignStart="@+id/textViewNumber"
        android:layout_below="@+id/textViewNumber"
        android:layout_marginTop="20dp"
        android:text="TextView"
        android:textSize="4sp" />

    <TextView
        android:id="@+id/textViewTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textViewID"
        android:layout_alignStart="@+id/textViewID"
        android:layout_below="@+id/textViewID"
        android:layout_marginTop="19dp"
        android:text="TextView"
        android:textSize="4sp" />

</RelativeLayout>